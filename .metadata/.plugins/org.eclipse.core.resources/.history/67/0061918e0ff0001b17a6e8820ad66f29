package day05_loop;

public class Q01 {
	public static void main(String[] args) {
		//1. hello 4번 출력
		for(int i=1; i<=4; i++) {
			System.out.println("hello");
		}
		
		//2. 1~100까지 합 중 그 합이 528일때 i 값 출력
		int sum=0;
		for(int i=1; i<=100; i++) {
			sum+=i;
			System.out.println(sum+"/"+i);
			if (sum==528) {
				break;
			}
		}
		
		//3. 
		for(int i=1; i<=25; i++) {
			
		}
		
		
	}
}
