package day06_while;

public class Test3 {
//1~1000까지 합을 구할때 
//순수한 3의 배수는 합에서 제외, 
//이때 5의 배수는 제외하지 않는다
//3만 얼마
	public static void main(String[] args) {
		int cnt=1, sum=0;
		while(1<=cnt&&cnt<=1000) {
			cnt++;
			if(cnt%3==0) {
				cnt++;
				if(cnt%5==0) {
					
				}
			}
			sum+=cnt;
			System.out.println(sum);
		}
	}
}
