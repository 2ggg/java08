package day03_if;

import java.util.Scanner;

public class Q03 {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("세 수 입력:");
		int a,b,c;
		a = scanner.nextInt();
		b = scanner.nextInt();
		c = scanner.nextInt();
		System.out.printf("%d, %d, %d", a,b,c);
	}
}
