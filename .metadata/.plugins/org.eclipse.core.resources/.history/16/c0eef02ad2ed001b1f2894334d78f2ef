package day02_variable;

import java.util.Scanner;

public class Q02 {
	private void rep() {
		for(int i=0; i<=15; i++) {
			System.out.print("=");
		}
		System.out.println();
	}
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		Q02 q02 = new Q02();
		q02.rep();
		int lang, eng, math;
		String name;
		System.out.print("이름: ");
		
		System.out.print("국어: ");
		System.out.print("영어: ");
		System.out.print("수학: ");
		
	}
}
