package day04_If;

import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a,b,c,max,min;
		System.out.print("세 수 입력: ");
		a = scanner.nextInt();
		b = scanner.nextInt();
		
		if(a>b) {
			max=a;
		}else if(a<b) {
			max=b;
		}
		if (max>c) {
			System.out.println("가장 큰 수는: "+max);
		}

	}

}
