package day04_If;

import java.util.Scanner;

public class Q01 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("날짜 입력>>");
		int date = scanner.nextInt();
		int result = date%7;
		switch (result) {
		case 1:
			
			break;

		default:
			break;
		}
	}
}
