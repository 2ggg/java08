package Juhee0729;

import java.util.Scanner;

public class Q05_juhee {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("별 개수 입력>>");
		int num=scanner.nextInt();
		int i=1;
		while(i<=num) {
			for(i=1; i<=num; i++) {
				
				System.out.print("*");
			}
		}
		
	}
}
